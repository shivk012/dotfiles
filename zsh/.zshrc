td(){
    first_arg=$1
    if [[ ""$first_arg"" =~ ^[A-Z] ]]
    then
        shift
        todo.sh add $@ | grep '\d*\s' -m 1 -o | xargs -I '{}' todo.sh pri '{}' $first_arg
    else
        todo.sh $@
    fi
}


